{
  "name": "lectures-scheduler",
  "version": "2.0.0",
  "description": "Application to schedule lectures",
  "repository": "github:Kyczan/lectures-scheduler",
  "main": "app.js",
  "scripts": {
    "setup": "rm -rf ./node_modules && npm i",
    "build": "rimraf ./dist && babel ./src -d ./dist --copy-files",
    "prestart": "npm run -s build",
    "start": "node dist/app.js",
    "dev": "nodemon -w ./src --exec \"babel-node ./src/app\"",
    "dev:clean": "npm run setup && npm run dev"
  },
  "keywords": [],
  "author": "krystian@kowal.pro",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "joi": "^13.3.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "promise-mysql": "^3.3.1",
    "pug": "^2.0.3",
    "pug-bootstrap": "0.0.15"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.17.4",
    "rimraf": "^2.6.2"
  }
}
