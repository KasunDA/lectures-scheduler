{
  "name": "lectures-scheduler",
  "version": "2.0.0",
  "description": "Application to schedule lectures",
  "repository": "github:Kyczan/lectures-scheduler",
  "main": "app.js",
  "scripts": {
    "build": "rimraf ./dist && babel ./src -d ./dist --copy-files && webpack -d",
    "build:watch": "npm run build -- --watch",
    "lint": "esw webpack.config.babel.js src --color",
    "lint:watch": "npm run lint -- --watch",
    "nodemon": "npm run build && nodemon -w ./src --exec \"node ./dist/app\"",
    "start": "npm-run-all --parallel build:watch lint:watch nodemon"
  },
  "keywords": [],
  "author": "krystian@kowal.pro",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "joi": "^13.3.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "promise-mysql": "^3.3.1",
    "pug": "^2.0.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.1.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-watch": "^4.0.1",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  }
}
